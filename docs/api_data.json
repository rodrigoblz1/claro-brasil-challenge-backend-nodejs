[  {    "type": "delete",    "url": "/devices/:id",    "title": "Deleta um device pelo seu id",    "name": "DeleteDevice",    "group": "Device",    "version": "0.0.1",    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "message",            "description": "<p>Informações referentes ao resultado da operação.</p>"          },          {            "group": "Success 200",            "type": "Object",            "optional": false,            "field": "device",            "description": "<p>Objeto que representa o device atualizado</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "{\n    \"message\": \"Device iPhone Novo was removed successfully\"\n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "BadRequest",            "description": "<p>A requisição não pode ser processada.</p>"          }        ]      },      "examples": [        {          "title": "Error-Response:",          "content": " HTTP/1.1 400 Bad Request\n{\n    \"message\": \"You can't delete your last device because you will not be able to add a new one at the moment.\n    You can register a new device only after Fri Mar 23 2018 10:05:48 GMT-0300 (-03)\"\n}",          "type": "json"        }      ]    },    "filename": "controllers/devices.js",    "groupTitle": "Device"  },  {    "type": "post",    "url": "/devices/",    "title": "Registra um novo device",    "name": "RegisterDevice",    "group": "Device",    "version": "0.0.1",    "header": {      "fields": {        "Header": [          {            "group": "Header",            "optional": false,            "field": "Content-Type",        